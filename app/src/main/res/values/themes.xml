<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FinalProject" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color2</item>
        <item name="colorPrimaryDark">@color/color2</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:typeface">monospace</item>
        <item name="fontFamily">@font/ar_font</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ThemeOverlay.AppCompat.Dark.ActionBar" parent="Base.ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="dialog_congratulation_animation">
        <item name="android:windowEnterAnimation">@anim/dialog_congratulation_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_congratulation_exit</item>
    </style>
</resources>